{
  "extends": "@tsconfig/node16-strictest/tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "noEmit": true,
    "checkJs": false,
    "noPropertyAccessFromIndexSignature": false,
    "exactOptionalPropertyTypes": false
  },
  "ts-node": {
    "transpileOnly": true,
    "require": ["dotenv/config"]
  },
  "exclude": ["node_modules", "dist"]
}
