{
  "extends": "@tsconfig/node16-strictest",
  "compilerOptions": {
    "outDir": "dist",
    "noEmit": true,
    "checkJs": false,
    "noPropertyAccessFromIndexSignature": false,
    "exactOptionalPropertyTypes": false,
    "ignoreDeprecations": "5.0"
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
